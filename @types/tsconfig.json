{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "./",
    "sourceMap": false,
    "target": "es2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "removeComments": false,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "esnext"
    ],
    "strict": true,
    "noImplicitAny": true
  }
}
